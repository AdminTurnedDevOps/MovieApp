<Project Sdk="Microsoft.NET.Sdk.Web">
    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
        <!-- The following four lines are for NuGet packaging ('nuget pack' command) -->
        <PackageId>MovieApp</PackageId>
        <Version>$VERSIONHERE$</Version>
        <Authors>Adam Bertram</Authors>
        <Company>Adam the Automator, LLC</Company>
        <!-- <GeneratePackageOnBuild>true</GeneratePackageOnBuild> Could use this here to automatically build but replacing in pipeline instead. -->
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.App" />
        <PackageReference Include="Microsoft.AspNetCore.Razor.Design" Version="2.2.0" PrivateAssets="All" />
        <PackageReference Include="Microsoft.EntityFrameworkCore.SQLite" Version="2.2.4" />
        <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="2.2.3" />
        <PackageReference Include="Pomelo.EntityFrameworkCore.MySql" Version="2.2.0" />
    </ItemGroup>
</Project>